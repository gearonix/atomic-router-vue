{
  "name": "atomic-router-vue",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.3",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/gearonix/atomic-router-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gearonix/atomic-router-vue.git"
  },
  "bugs": "https://github.com/gearonix/atomic-router-vue/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve playground",
    "dev:build": "vite build playground",
    "dev:preview": "vite preview playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "prepare": "simple-git-hooks",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "atomic-router": "^0.10.0",
    "effector": "^22.8.8 || ^23",
    "effector-vue": "^21.0.0",
    "react": "^17 || ^18",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "atomic-router": "^0.10.1",
    "effector": "^23.2.0",
    "effector-vue": "^23.0.0",
    "eslint": "^8.56.0",
    "history": "^5.3.0",
    "lint-staged": "^15.2.2",
    "mkdist": "^1.4.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "2.0.0",
    "vite": "^5.1.3",
    "vitest": "^1.3.0",
    "vue": "^3.4.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm typecheck"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
